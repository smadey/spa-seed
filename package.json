{
  "name": "spa-seed",
  "description": "Single page seed project",
  "author": "Smadey <smadey@163.com>",
  "private": false,
  "scripts": {
    "dev": "webpack-dev-server --inline --hot --port 8080",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "init": "npm run clear:libs && npm run init:libs",
    "clear:libs": "git rm -r lib-core && git rm -r lib-components && git add . && git commit -m \"Remove libs for init\"",
    "init:libs": "npm run init:core && npm run init:components",
    "init:core": "git remote add -f remote-lib-core https://github.com/smadey/lib-core && git subtree add --prefix=lib-core remote-lib-core master",
    "init:components": "git remote add -f remote-lib-components https://github.com/smadey/vue-components && git subtree add --prefix=lib-components remote-lib-components master",
    "push:core": "git subtree push --prefix=lib-core remote-lib-core master",
    "push:components": "git subtree push --prefix=lib-components remote-lib-components master"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smadey/radarwin"
  },
  "license": "MIT",
  "dependencies": {
    "promise": "^7.1.1",
    "vue": "^1.0.22",
    "vue-router": "^0.7.13",
    "whatwg-fetch": "^0.11.0",
    "ylib": "^0.1.6"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-runtime": "^6.6.1",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "eslint": "^2.9.0",
    "eslint-config-airbnb-base": "^3.0.0",
    "eslint-friendly-formatter": "^2.0.4",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-html": "^1.4.0",
    "eslint-plugin-import": "^1.7.0",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
    "node-sass": "^3.7.0",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "vue-hot-reload-api": "^1.3.2",
    "vue-html-loader": "^1.2.2",
    "vue-loader": "^8.3.1",
    "vue-style-loader": "^1.0.0",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  }
}
